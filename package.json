    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-sort-exports": "0.4.0",
    "eslint-plugin-unused-imports": "1.1.2",
    "faker": "5.5.3",
    "memory-streams": "0.1.3",
    "mocha": "9.0.3",
    "mocha-lcov-reporter": "1.3.0",
    "nyc": "15.1.0",
    "rimraf": "3.0.2",
    "sinon": "11.1.2",
    "ts-node": "10.1.0",
    "typescript": "4.3.5"
  }
