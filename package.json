    "chalk": "4.1.2",
    "cli-table": "0.3.6",
    "humanize-duration": "3.27.0",
    "minimist": "1.2.5",
    "node-cache": "5.1.2",
    "parse-duration": "1.0.0",
    "string-format": "2.0.0",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@types/chai": "4.2.21",
    "@types/chai-as-promised": "7.1.4",
    "@types/mocha": "9.0.0",
    "@types/node": "16.4.10",
    "@types/sinon": "10.0.3",
    "@types/string-format": "2.0.0",
